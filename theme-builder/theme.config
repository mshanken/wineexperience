
/*******************************
        Theme Selection
*******************************/

/* To override a theme for an individual element
   specify theme name below
*/

/* Default Theme */
// Reset and dependencies and core default theme
// Globals
@normalize              : 'default';
@print                  : 'default';
@icons                  : 'fontawesome';// (default/bootstrap/fontawesome/fuelux)
@colors                 : 'default';
@scaffolding            : 'default';
@type                   : 'default';
@utilities              : 'bootstrap';  // (default/bootstrap)
@responsive-utilities   : 'bootstrap';  // (default/bootstrap/fuelux)
@grid                   : 'default';    // 
@code                   : 'default';
@tables                 : 'default';
@forms                  : 'default';    // (default/fuelux)
@buttons                : 'default';


// Components
@component-animations   : 'fontawesome';  // (default/bootstrap/fontawesome)
@cards                  : 'bootstrap';  // (default/bootstrap)
@dropdowns              : 'bootstrap';  // (default/bootstrap)
@button-groups          : 'bootstrap';  // (default/bootstrap)
@input-groups           : 'bootstrap';  // (default/bootstrap)
@navs                   : 'bootstrap';  // (default/bootstrap/jasny)
